      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "4300"
      ]
    },
    {
      "name": "Flutter Web Fixed Port Mock Data",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "4300",
        "--dart-define=USE_MOCK_DATA=true"
      ]
    }
  ]
}
